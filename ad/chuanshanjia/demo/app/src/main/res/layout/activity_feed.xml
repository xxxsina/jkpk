<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <Button
        android:id="@+id/btn_FD_back"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/title"
        android:drawableStart="@drawable/arror_foreground"
        android:drawablePadding="2dp"
        android:gravity="left|center_vertical"
        android:paddingLeft="13dp"
        android:text="BytedanceUnion Demo"
        android:textAllCaps="false"
        android:textColor="@color/white"
        android:textSize="15sp" />

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginTop="20dp"
        android:background="#E3E2E2" />

    <Button
        android:id="@+id/btn_main_feed_lv"
        android:layout_width="match_parent"
        android:layout_height="52dp"
        android:background="#fff"
        android:drawableStart="@drawable/demo_native"
        android:drawableEnd="@drawable/draw_forward"
        android:drawablePadding="20dp"
        android:gravity="start|center_vertical"
        android:paddingLeft="12dp"
        android:paddingRight="12dp"
        android:stateListAnimator="@null"
        android:text="@string/main_menu_feed_list_view"
        android:textAllCaps="false"
        android:textColor="#000"
        android:textSize="14sp"
        android:theme="@style/PrimaryButton" />

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginStart="15dp"
        android:layout_marginLeft="15dp"
        android:background="#E3E2E2" />

    <Button
        android:id="@+id/btn_main_feed_rv"
        android:layout_width="match_parent"
        android:layout_height="52dp"
        android:background="#fff"
        android:drawableStart="@drawable/demo_native"
        android:drawableEnd="@drawable/draw_forward"
        android:drawablePadding="20dp"
        android:gravity="left|center_vertical"
        android:paddingLeft="12dp"
        android:paddingRight="12dp"
        android:stateListAnimator="@null"
        android:text="@string/main_menu_feed_recycler_view"
        android:textAllCaps="false"
        android:textColor="#000"
        android:textSize="14sp"
        android:theme="@style/PrimaryButton" />


    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#E3E2E2" />

    <Button
        android:id="@+id/express_native_ad"
        android:layout_width="match_parent"
        android:layout_height="52dp"
        android:background="#fff"
        android:drawableStart="@drawable/demo_native"
        android:drawableEnd="@drawable/draw_forward"
        android:drawablePadding="20dp"
        android:gravity="start|center_vertical"
        android:paddingLeft="12dp"
        android:paddingRight="12dp"
        android:stateListAnimator="@null"
        android:text="@string/main_menu_feed_webview"
        android:textAllCaps="false"
        android:textColor="#000"
        android:textSize="14sp"
        android:theme="@style/PrimaryButton" />

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#E3E2E2" />

    <Button
        android:id="@+id/express_native_ad_list"
        android:layout_width="match_parent"
        android:layout_height="52dp"
        android:background="#fff"
        android:drawableStart="@drawable/demo_native"
        android:drawableEnd="@drawable/draw_forward"
        android:drawablePadding="20dp"
        android:gravity="start|center_vertical"
        android:paddingLeft="12dp"
        android:paddingRight="12dp"
        android:stateListAnimator="@null"
        android:text="@string/main_menu_feed_webview_list"
        android:textAllCaps="false"
        android:textColor="#000"
        android:textSize="14sp"
        android:theme="@style/PrimaryButton" />



    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#E3E2E2" />

    <Button
        android:id="@+id/express_native_icon_ad"
        android:layout_width="match_parent"
        android:layout_height="52dp"
        android:background="#fff"
        android:drawableLeft="@drawable/demo_native"
        android:drawableRight="@drawable/draw_forward"
        android:drawablePadding="20dp"
        android:gravity="left|center_vertical"
        android:paddingLeft="12dp"
        android:paddingRight="12dp"
        android:stateListAnimator="@null"
        android:text="@string/main_menu_feed_icon_ad"
        android:textAllCaps="false"
        android:textColor="#000"
        android:textSize="14sp"
        android:theme="@style/PrimaryButton" />

    <Button
        android:id="@+id/native_btn_ec_mall"
        android:layout_width="match_parent"
        android:layout_height="52dp"
        android:background="#fff"
        android:drawableLeft="@drawable/demo_native"
        android:drawableRight="@drawable/draw_forward"
        android:drawablePadding="20dp"
        android:gravity="left|center_vertical"
        android:paddingLeft="12dp"
        android:paddingRight="12dp"
        android:stateListAnimator="@null"
        android:text="@string/main_menu_feed_ec_mall"
        android:textAllCaps="false"
        android:textColor="#000"
        android:textSize="14sp"
        android:theme="@style/PrimaryButton" />


</LinearLayout>