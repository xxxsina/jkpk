<?xml version="1.0" encoding="utf-8"?>
<com.kwad.sdk.core.view.AdBaseFrameLayout android:id="@+id/ksad_web_card_frame"
  xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:tools="http://schemas.android.com/tools"
  android:layout_width="match_parent"
  android:layout_height="match_parent"
  android:background="#00000000">

  <com.kwad.sdk.core.webview.KsAdWebView
    android:id="@+id/ksad_web_card_webView"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#00000000"
    android:scrollbarStyle="insideOverlay" />

  <ImageView
    android:id="@+id/ksad_end_close_btn"
    android:layout_width="28dp"
    android:layout_height="28dp"
    android:layout_alignParentRight="true"
    android:layout_gravity="right|top"
    android:layout_marginRight="16dp"
    android:layout_marginTop="16dp"
    android:scaleType="centerInside"
    android:src="@drawable/ksad_page_close"
    android:visibility="visible"
    tools:visibility="visible" />
</com.kwad.sdk.core.view.AdBaseFrameLayout>
